{
  "name": "@manzano/etag-middleware",
  "module": "index.ts",
  "type": "module",
  "version": "1.0.4",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "make": "bun build ./index.ts --outdir ./build --target node && bun type:declaration",
    "deploy": "npm publish --access public",
    "prettier:check": "npx prettier . --check",
    "prettier:write": "npx prettier . --write",
    "type:declaration": "tsc --declaration index.ts --outDir build"
  },
  "devDependencies": {
    "@types/node": "20.9.0",
    "bun-types": "1.0.11",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  }
}
